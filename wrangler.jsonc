{
    "$schema": "node_modules/wrangler/config-schema.json",
    "main": "./dist/_worker.js/index.js",
    // Update to today's date
    "compatibility_date": "2025-03-25",
    "compatibility_flags": ["nodejs_compat"],
    "keep_vars": true,
    "assets": {
      "binding": "ASSETS",
      "directory": "./dist"
    },
    "observability": {
      "enabled": true
    }
}